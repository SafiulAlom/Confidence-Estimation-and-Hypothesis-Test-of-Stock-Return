#Funktion zur Berechnung von Konfidenzintervalle f√ºr Erfolgswahrscheinlichkeit
KI.Binom<-function (PD,niveau, n )
{
#untere Inervallgrenze
KI.Unter <- PD - round(qnorm(1-(niveau/2)),2)*sqrt(PD*(1-PD)/n)
#untere Inervallgrenze
KI.Ober <- PD + round(qnorm(1-(niveau/2)),2)*sqrt(PD*(1-PD)/n)
return(c(KI.Unter, KI.Ober))
}
KI.Binom(0.35, 0.05, 500)
